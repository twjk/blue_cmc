<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!-- 用于参数传递特殊字符 如配置管理中的cfgMap['fee.freelang'] -->	
	<package name="sp" extends="struts-custom" >
		<!-- 自定义拦截器 -->
		<interceptors>
        	<interceptor-stack name="myDefaultStack">
            	<interceptor-ref name="defaultStack">
					<!-- 字母.字母_数字 或 字母[数字] 或 字母(数字) 或 字母['字母数字_.-|'] 或 字母('字母_数字') -->
                	<param name="params.acceptParamNames">\w+((\.\w+)|(\[\d+\])|(\(\d+\))|(\['[\w\.\-|]+'\])|(\('\w+'\)))*</param>
            	</interceptor-ref>
        	</interceptor-stack> 
		</interceptors>
		<default-interceptor-ref name="myDefaultStack"/>
		
		<!-- 能力维护 -->
		<action name="funcCapAction" class="com.qcmz.cmc.web.action.FuncCapAction">
			<result name="lang">/datamgr/func/funcProxyLang.jsp</result>
			<result name="langlang">/datamgr/func/funcProxyLanglang.jsp</result>
		</action>
		
		<!-- 修改配置 -->
		<action name="configAction" class="com.qcmz.cmc.web.action.ConfigAction" />
	</package>
</struts>